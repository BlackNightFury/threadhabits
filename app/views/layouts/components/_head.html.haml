%title
  = content_for(:title) ? yield(:title) : "Threadhabits - Buying and selling menswear."

= csrf_meta_tags
-#- cache "v31" do
= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
= javascript_include_tag 'application', 'data-turbolinks-track': 'reload'

= favicon_link_tag "#{S3_ASSET_PATH}/threadhabitsnewfavicon.png"
= include_google_map(places: true)
%link{:href => "https://fonts.googleapis.com/css?family=Lato:100,700", :rel => "stylesheet", :type => "text/css"}

%meta{:name => "viewport", :content => "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"}
